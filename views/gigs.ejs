<h1>All Projects</h1>

<% gigs.forEach(function(gig){ %>
<div class="card text-white bg-secondary mb-3" style="max-width: 20rem">
  <div class="card-header"><%= gig.title %></div>
  <div class="card-body">
    <h6 class="card-title">Technologies: <%= gig.technologies %></h6>
    <p class="card-text">Project Decription: <%= gig.description %></p>
    <p class="card-text">Contact Information: <%= gig.contact %></p>
  </div>
</div>
<% }) %>
